{
	"includeFiles": [
		"Neto.ImportOption.json",
		"..\\NameMap.json",
		"NetO_Classification.json"
	],
	"sourceFile": "NetOPhysicalModel-20221122.csv",
	"buildFolder": null,
	"tableTypeMetadata": "ConsASAP_MetaData_20221122.csv",
	"uspLoadTableOption": {
		"outputFile": "neto_usp_LoadTable_meta.sql",
		"dataTableName": "etl.ETLGenPrimaryKeyColumns",
		"dataLayerName": "clt_neto"
	},
	"rawToCultivated": {
		"outputFile": "cntrl_Neto_raw_to_cultivated_copyinto_meta.sql",
		"pipelineName": "pl_Neto_raw_to_cultivated_copyinto",
		"activityName": "cp_extract_to_raw",
		"paramValueFormat": "Netoxygen_*_20"
	},
	"updateCommands": [
		"excludeView += WG_RPT*.*",
		"excludeView -= WG_RPT_LOAN_STATUS.*",
		"primaryKey += WG_SYMBOL_XREF.TABLENAME",
		"primaryKey += WG_SYMBOL_XREF.COLUMNNAME",
		"primaryKey += WG_SYMBOL_XREF.FLDNAME",
		"primaryKey += L_SYMBOL.FLDNAME",
		"primaryKey += L_SYMBOL.DBSYMBOL",
		"copy += Include\\VwSymbolLookup.sql => clt_NetO\\Views\\VwSymbolLookup.sql"
	],

	"Relationships": [
		{
			"TableName": "GF_TS_AUDIT_LOAN_DELETE",
			"ColumnName": "DELETED_LNUM",
			"ReferenceObjectId": "*.LNUM"
		}
	],
	"lookupRelationships": [
		{
			"pattern": "*.S_*",
			"selectLine": "{alias}.Descript AS [{viewColumnName}_Description]",
			"joinLine": "LEFT JOIN [clt_NetO].[SymbolLookup] {alias} on x.{viewColumnName} = {alias}.DBSYMBOL AND {alias}.[TableName] = '{tableName}' and {alias}.[COLUMNNAME] = '{viewColumnName}'"
		}
	],
	
	"addInstructions": [
		{
			"pattern": "*.LNUM",
			"where": "NOT EXISTS (SELECT * FROM [clt_NetO].[GF_TS_AUDIT_LOAN_DELETE] i WHERE x.[LNUM] = i.[DELETED_LNUM])"
		},
		{
			"pattern": "*.S_*",
			"selectLine": "{alias}.Descript AS [{viewColumnName}_Description]",
			"joinLine": "LEFT JOIN [clt_NetO].[SymbolLookup] {alias} on x.{viewColumnName} = {alias}.DBSYMBOL AND {alias}.[TableName] = '{tableName}' and {alias}.[COLUMNNAME] = '{viewColumnName}'"
		}
	]
}